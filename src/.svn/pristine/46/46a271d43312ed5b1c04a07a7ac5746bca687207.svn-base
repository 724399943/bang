/*index*/
.in-head {height: 39px;line-height: 39px;position: fixed;top: 0px;left: 0;width: 100%;z-index:9;color:#fff;}
.in-head.inbg{background:rgba(0,0,0,.1);}
.recoverbg{animation:recoverbg 3s forwards;-webkit-animation:recoverbg 3s forwards;}
.changebg{animation:changebg 3s forwards;-webkit-animation:changebg 3s forwards;}
@keyframes changebg{0%{background:rgba(0,0,0,.1);}100%{background:rgb(79,160,244);}}
@-webkit-keyframes changebg{0%{background:rgba(0,0,0,.1);}100%{background:rgb(79,160,244);}}
@keyframes recoverbg{0%{background:rgb(79,160,244);}100%{background:rgba(0,0,0,.1);}}
@-webkit-keyframes recoverbg{0%{background:rgb(79,160,244);}100%{background:rgba(0,0,0,.1);}}
