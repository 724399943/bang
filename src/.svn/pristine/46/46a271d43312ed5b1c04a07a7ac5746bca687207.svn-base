.content{overflow-x:hidden;}
.main{padding-top:40px;}
.navtop{padding-top:80px;}
.pd{padding-bottom:50px;}
.plg{padding:40px 15px 0px 15px;}
.nomore{width:100%;line-height:45px;text-align:center;}

/*nav*/
.head {height: 39px;line-height: 39px;position: fixed;top: 0px;left: 0;width: 100%;z-index:9;border-bottom:1px solid #dbdbdb;background:#fff;}
.head .ico {height: 100%;width: 55px;display: block;position: absolute;top: 0px;left: 0;text-align:right;}
.y-confirm-order-h1 {display: block;height: 100%;text-align: center;font-size: 17px;color: #333;padding:0 60px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.head .text{line-height:40px;text-align:center;color:#333;position:absolute;top:0;right:10px;}

/*footer*/
.y-ycar-foot {height: 49px;background: #fff;bottom: 0;position: fixed;left: 0;right: 0;border-top:1px solid #e4e4e4;z-index:999999;}
.y-ycar-foot a {width: 20%;display: block;height: 100%;float: left;position:relative;}
.y-ycar-foot a .icon {display: block;padding-top: 30px;height: 20px;line-height: 20px;color: #666666;font-size: 12px;text-align: center;position: relative;}
.y-ycar-foot .icon i {position: absolute;top: 12.5px;left: 50%;right: 0;display: block;width: 25px;height: 25px;margin-left: -12.5px;}
.y-ycar-foot .icon i.release{top:5px;width:40px;height:40px;margin-left:-20px;}
.y-ycar-foot a:nth-child(1) i {background: url(../images/home_table_house.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(2) i {background: url(../images/home_table_classfiy.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(3) i {background: url(../images/home_table_release.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(4) i {background: url(../images/home_table_chat.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(5) i {background: url(../images/home_table_me.png) center center no-repeat;background-size: contain;}
/*.y-ycar-foot a.on .icon {color: #f8a411;}*/
.y-ycar-foot a.on:nth-child(1) i {background:url(../images/home_table_house_on.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(2) i {background:url(../images/home_table_classfiy_on.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(3) i {background:url(../images/home_table_release.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(4) i {background:url(../images/home_table_chat_on.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(5) i {background:url(../images/home_table_me_on.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a .relinput{position:absolute;top:0;left:0;width:100%;height:100%;opacity:0;z-index:9999;display:block;}

/*过渡动画*/
.fade-slide-enter-active, .fade-slide-leave-active {width: 100%;height:100%;top:0px;position:absolute;transition: all 300ms;}
.fade-slide-enter{opacity:0; transform: translate3d(100%, 0, 0);}
.fade-slide-leave-to{opacity: 0;}

.fade-enter-active, .fade-leave-active {transition: opacity .5s;}
.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {opacity: 0;}
.touchleft{transform: translateX(-58px);-webkit-transform: translateX(-58px);-moz-transform: translateX(-58px);}

.slideUp{transform:translate3d(0,0,0);-webkit-transform:translate3d(0,0,0);opacity:1;}
.slideDown{transform:translate3d(0,100%,0);-webkit-transform:translate3d(0,100%,0);opacity:0;}

/*login*/
.head .back{background: url(../images/icon_back.png) no-repeat center center;background-size: 43%;}
.login_logo{display:block;width:120px;margin:10% auto 15%;}
.login_input {height: 50px;margin:0 auto;width:93%;position: relative;border-bottom:1px solid #dbdbdb;}
.login_input .name{color:#394456;margin-right:15px;width:43px;display:inline-block;}
.login_input input {height: 50px;line-height: 50px;border: none;color:#666;}
.login_input em{display:block;width:23px;height:100%;background:url(../images/login_input_icon_n.png) center center no-repeat;background-size:contain;position:absolute;top:0;right:15px;}
.login_input em.on{background:url(../images/login_input_b.png) center center no-repeat;background-size:contain;}
.linput {padding:0 15px;margin:0 auto;height:50px;line-height:50px;font-size:12px;overflow:hidden;color:#999;}
.linput .lset{float:left;}
.linput .lpass{float:right;}
.login_btn {width:80%;height: 40px;background: #4f9bf2;color: #fff;display: block;text-align: center;line-height: 40px;margin: 60px auto 0;border-radius: 5px;font-size: 16px;}
.cooperative-login{position:absolute;bottom:0;left:0;width:100%;padding:15px 0;}
.cooperative-login .pt{color:#666666;text-align:center;margin-bottom:15px;}
.cooperative-login .box {overflow: hidden;text-align:center;}
.cooperative-login .box .l-g-b {display:inline-block;margin:0 5%;}
.cooperative-login .box .l-g-b img{width:35px;height:35px;}
.login_tips{width:100%;position:absolute;top:0;left:0;line-height:40px;color:#fff;background:#494949;text-align:center;z-index:10;font-size:12px;}
.login-alert{position:absolute;top:0;left:0;right:0;bottom:0;background:rgba(0,0,0,.5);z-index:999;}
.login-alertBox{width:70%;background:#f1f1f1;border-radius:10px;position:absolute;top:50%;left:15%;transform:translateY(-50%);-webkit-transform:translateY(-50%);-moz-transform:translateY(-50%);-ms-transform:translateY(-50%);-o-transform:translateY(-50%);}
.login-alertBox .txt{text-align:center;padding:25px 0;overflow:hidden;white-space:nowrap;}
.login-alertBox .btnBox{display:-webkit-box;color:#999999;text-align:center;border-top:1px solid #dfdfe1;}
.login-alertBox .btnBox a{display:block;width:50%;line-height:40px;}
.login-alertBox .btnBox a.reg{color:#0076ff;border-left:1px solid #dfdfe1;}
.login_input .codeBtn{display:block;position:absolute;top:-1px;right:-3.5%;height:52px;line-height:52px;text-align:center;color:#fff;background:#4f9bf2;padding:0 10px;}
.login_input .codeBtn.res{background:#dfdfe1;}
.agree{line-height:50px;width:93%;margin:0 auto;color:#999999;font-size:12px;}
.agree .txt{color:#4f9bf2;}
.reg_tips{width:130px;padding:15px 0;border-radius:10px;background:rgba(0,0,0,.8);z-index:999;position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);}
.reg_tips .ico{width:50%;display:block;margin:0 auto;}
.reg_tips .pt{color:#fff;text-align:center;margin-top:10px;}

.agreementWrap{padding:0 15px;}

/*selectUserType*/
.usertype .type{display:block;margin:15% auto;width:45%;}
.usertype .type img{width:100%;}

/*authentication*/
.y-bindphone ul li{background:#fff;padding:5px 10px;height:35px;line-height:35px;color:#666;position:relative;border-bottom:1px solid #e4e4e4;transition:all 0.5s;}
