.uhpdata .paper{overflow:hidden;margin-left:-10px;margin-top:10px;}
.uhpdata .paper>span{float:left;text-align:center;color:#4f9bf2;border:1px solid #4f9bf2;border-radius:5px;padding:5px 10px;margin-left:10px;margin-bottom:10px;}
.uhpCont .uhpNav{border-bottom:1px solid #e6ebee;display:flex;display:-webkit-flex;justify-content:space-between;flex-flow:nowrap;padding:0 15%;}
.uhpCont .uhpNav>span{position:relative;padding-bottom:11px;}
.uhpCont .uhpNav>span.on:before{content:"";display:block;width:90%;height:2px;border-radius:15px 15px;background:#4f9bf2;position:absolute;bottom:8px;left:50%;margin-left:-45%;}
.uhpCont .uhp-demand{padding:0 15px;}
.msgBoard.uhpeva{padding:15px 0;}
.msgBoard.uhpeva .hpd{padding:0 15px;}
.odetul>li .uhptext{width:75%;font-size:12px;text-align:right;}
.odetul.pdnone{padding:0;}
.uhpintroduce .title{padding:10px 0;}

/*choicebox*/
.choiceBox{width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,.5);z-index:9999;}
.choiceBox .choice-wrap{width:70%;position:absolute;bottom:0;left:50%;margin-left:-35%;transition:all 0.25s;}
.choiceBox .choice-wrap .opction{border-radius:10px;background:#fff;}
.choiceBox .choice-wrap .opction .pt{text-align:center;line-height:40px;}
.choiceBox .choice-wrap .opction .pt.on{color:#4f9bf2;}
.choiceBox .choice-wrap .opction .pt:nth-child(1){border-bottom:1px solid #e4e4e4;}
.choiceBox .choice-wrap .btn{text-align:center;background:#fff;border-radius:10px 10px;line-height:40px;margin:15px 0;}

/*addlabels*/
.lab-img img{width:100%;height:100%;}
.c-classfiy.clpd{padding:15px;}
.c-classfiy.clpd ul .clli{border-color:#4f9bf2;color:#4f9bf2;}
.c-classfiy.clpd li .closed{width:17px;height:17px;display:block;background:url(../images/home-icon-del.png) center center no-repeat;background-size:contain;position:absolute;top:-8px;right:-7px;}